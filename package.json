{
  "name": "counter-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/reactappnpmdemo",
  "dependencies": {
    "@mstechusa/eviewer7": "^7.0.136",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-pdf": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-uuid": "^1.0.2",
    "typescript": "^4.2.3",
    "uuid": "^8.2.0",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts --max_old_space_size=10000 start",
    "build": "react-scripts --max_old_space_size=10000 build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@mstechusa/eviewer7)/\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": "off",
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.22.0"
  }
}
